<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="RCONServerIP" value="XXX.XXX.XXX.XXX" />
    <add key="RCONServerPort" value="47200" />
    <!-- Default RCON port is 47200 for Battlefield 3-->
    <add key="RCONServerPassword" value="XXXX"/>
    <add key="WebservicePort" value="XXXX"/>
    <!-- Webservice port need to be open in your router if you whant to connect with your phone over internet, but network works also -->
    <add key="WebserviceAdminUsername" value="admin"/>
    <add key="WebserviceAdminPassword" value="XXXXX"/>
    <!-- Authentications to get to the web interface -->
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
</configuration>